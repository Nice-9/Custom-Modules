<odoo>
    <record id="ir_cron_generate_tracking_notifications" model="ir.cron">
        <field name="name">Daily Tracking Notifications</field>
        <field name="model_id" ref="crm_lead_tracking.model_crm_lead_tracking_log"/>
        <field name="state">code</field>
        <field name="code">model._generate_daily_tracking_notifications()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="active">True</field>
    </record>
</odoo>
