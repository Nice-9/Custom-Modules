<odoo>
    <record id="view_crm_lead_tracking_log_tree" model="ir.ui.view">
        <field name="name">crm.lead.tracking.log.tree</field>
        <field name="model">crm.lead.tracking.log</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="date"/>
                <field name="user_id"/>
                <field name="lead_id"/>
                <field name="latitude"/>
                <field name="longitude"/>
               
            </tree>
        </field>
    </record>

    <record id="view_crm_lead_tracking_log_kanban" model="ir.ui.view">
        <field name="name">crm.lead.tracking.log.kanban</field>
        <field name="model">crm.lead.tracking.log</field>
        <field name="arch" type="xml">
            <kanban>
                <field name="name"/>
                <field name="date"/>
               
                <templates>
                    <t t-name="kanban-box">
                        <div class="oe_kanban_global_click">
                            <strong><field name="name"/></strong>
                            <div><field name="date"/></div>
                           
                        </div>
                    </t>
                </templates>
            </kanban>
        </field>
    </record>


    <record id="crm_lead_tracking_log_action" model="ir.actions.act_window">
        <field name="name">Lead Tracking Logs</field>
        <field name="res_model">crm.lead.tracking.log</field>
        <field name="view_mode">tree,kanban</field>
    </record>

    <menuitem id="crm_lead_tracking_log_menu_root" name="Lead Tracking" parent="crm.crm_menu_root"/>
    <menuitem id="crm_lead_tracking_log_menu" name="Tracking Logs"
              parent="crm_lead_tracking_log_menu_root" action="crm_lead_tracking_log_action"/>
</odoo>